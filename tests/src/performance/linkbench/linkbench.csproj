<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>LinkBench</AssemblyName>
    <OutputType>exe</OutputType>
    <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\..\</SolutionDir>
    <NugetTargetMoniker>.NETStandard,Version=v1.6</NugetTargetMoniker>
    <NugetTargetMonikerShort>netstandard1.6</NugetTargetMonikerShort>
    <TargetFramework>netstandard1.6</TargetFramework>
    <CLRTestKind>BuildOnly</CLRTestKind>
  </PropertyGroup>
  <PropertyGroup>
    <RestoreOutputPath>..\obj</RestoreOutputPath>
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <CodeAnalysisDependentAssemblyPaths Condition=" '$(VS100COMNTOOLS)' != '' " Include="$(VS100COMNTOOLS)..\IDE\PrivateAssemblies">
      <Visible>False</Visible>
    </CodeAnalysisDependentAssemblyPaths>
  </ItemGroup>
  <PropertyGroup>
    <ProjectAssetsFile>$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), performance.csproj))\obj\project.assets.json</ProjectAssetsFile>
  </PropertyGroup>
</Project>
